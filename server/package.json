{
  "name": "zenips-gaming-server",
  "version": "1.0.0",
  "description": "Backend API for Zenips Gaming. A blog for gamers by gamers.",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix ../client",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix ../client && npm run build --prefix ../client"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/EricPuskas/zenips-gaming-server.git"
  },
  "author": "Eric Puskas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricPuskas/zenips-gaming-server/issues"
  },
  "homepage": "https://github.com/EricPuskas/zenips-gaming-server#readme",
  "dependencies": {
    "async": "^2.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-form-data": "^2.0.7",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.8"
  }
}
